{
  "root": true,
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
    // "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
    // "prettier"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "semi": "error",
    "indent": ["warn", 2]
    // "prettier/prettier": 2 // Means error
  }
}
